<?xml version="1.0" encoding="UTF-8"?>
<MasterDetailPage
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="PrismPruebaArranque1.Views.MainPage">
    <MasterDetailPage.Master>
        <NavigationPage Title="Required Foo">
            <x:Arguments>
                <ContentPage Title="Menu">
                    <StackLayout Padding="40">
                        <Button Text="View A" Command="{Binding NavigateCommand}" CommandParameter="NavigationPage/ViewA" />
                        <Button Text="View B" Command="{Binding NavigateCommand}" CommandParameter="NavigationPage/ViewB" />
                    </StackLayout>
                </ContentPage>
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Master>
</MasterDetailPage>